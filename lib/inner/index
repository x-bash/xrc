# shellcheck shell=dash

if [ -z "$ZSH_VERSION" ]; then

___x_cmd_inner_cd(){
    builtin cd "$@" || return
}

___x_cmd_inner_mv(){
    builtin mv "$@" || return
}

else

___x_cmd_inner_cd(){
    cd "$@" || return
}

___x_cmd_inner_mv(){
    command mv "$@"
}

fi

___x_cmd_inner_awk(){
    xrc:mod xrc/lib/inner/awk
}

___x_cmd_inner_curl(){
    xrc:mod xrc/lib/inner/curl
}
